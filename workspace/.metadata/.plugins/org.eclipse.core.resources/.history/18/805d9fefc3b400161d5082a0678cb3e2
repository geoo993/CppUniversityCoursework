#include <iostream>
#include <vector>
#include <math.h>
#include <algorithm>
#include <sstream>
using namespace std;


template <typename Item>
class stack {

public:
	vector<Item> v;
    Item & top();

};


template <typename Item>
Item & stack<Item>::top() {
	return v.back();
}


int main() {

	vector<int> yy = {2, 5, 74, 12, 35};
	stack<int> sampl;
	sampl.v = yy;
	sampl.top();

	samplet<string> vc;

	return 0;
}
